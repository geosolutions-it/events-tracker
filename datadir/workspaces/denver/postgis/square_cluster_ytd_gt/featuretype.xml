        <name>square_cluster_ytd_gt</name>
        <sql>SELECT&#xd;
  count(1) count,&#xd;
  type,&#xd;
  x_round,&#xd;
  y_round,&#xd;
  st_MakePolygon(st_makeline(&#xd;
    ARRAY[&#xd;
	    ST_MakePoint(x_round - %r% / 2, y_round - %r% / 2),&#xd;
	    ST_MakePoint(x_round - %r% / 2, y_round + %r% / 2),&#xd;
	    ST_MakePoint(x_round + %r% / 2, y_round + %r% / 2),&#xd;
	    ST_MakePoint(x_round + %r% / 2, y_round - %r% / 2),&#xd;
	    ST_MakePoint(x_round - %r% / 2, y_round - %r% / 2)&#xd;
    ]&#xd;
 )) AS geom &#xd;
FROM (&#xd;
  SELECT&#xd;
    geom,&#xd;
    type,    &#xd;
    ST_X(geom) AS x,&#xd;
    ST_Y(geom) AS y,&#xd;
    round(ST_X(geom) / %r%) * %r% AS x_round,&#xd;
    round(ST_Y(geom) / %r%) * %r% AS y_round&#xd;
  FROM&#xd;
    denver.crime_location &#xd;
  WHERE&#xd;
   mo BETWEEN %mo_min% AND %mo_max%&#xd;
   AND type IN (%crime_type%)&#xd;
   :where_clause:&#xd;
) agg GROUP BY x_round, y_round, type
</sql>
